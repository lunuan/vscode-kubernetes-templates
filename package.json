{
    "name": "kubernetes-templates",
    "displayName": "Kubernetes Templates",
    "description": "Kubernetes Templates",
    "keywords": [
        "k8s",
        "kubernetes",
        "snippets",
        "templates",
        "yaml",
        "yml"
    ],
    "icon": "static/k8s-logo.png",
    "license": "MIT",
    "publisher": "lunuan",
    "maintainers": [
        "admin@lunuan.com.cn",
        "huaxing@lunuan.com.cn"
    ],
    "version": "1.3.0",
    "repository": {
        "type": "git",
        "url": "https://lunuan@dev.azure.com/lunuan/kubernetes.templates/_git/kubernetes.templates"
    },
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Snippets"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "snippets": [
            {
                "language": "yaml",
                "path": "./snippets/svc.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/cm.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/workload.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/secret.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/ep.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/ingress.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/pvc.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/limit_range.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/quota.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "@types/vscode": "^1.31.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^10.2.0",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "vsce": "^1.96.1"
    }
}
